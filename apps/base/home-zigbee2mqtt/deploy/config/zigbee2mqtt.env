ZIGBEE2MQTT_CONFIG_MQTT_SERVER=mqtts://mosquitto.home-mosquitto.svc.cluster.local:8883
ZIGBEE2MQTT_CONFIG_MQTT_CA=/etc/ssl/cluster/ca.cert
ZIGBEE2MQTT_CONFIG_SERIAL_PORT=tcp://1.2.3.4:3333
ZIGBEE2MQTT_CONFIG_SERIAL_ADAPTER=zstack
ZIGBEE2MQTT_CONFIG_PERMIT_JOIN=false
# Optional: Logging level, options: debug, info, warn, error (default: info)
ZIGBEE2MQTT_CONFIG_ADVANCED_LOG_LEVEL=debug
ZIGBEE2MQTT_CONFIG_ADVANCED_LOG_OUTPUT=["console"]
ZIGBEE2MQTT_CONFIG_FRONTEND_PORT=8080
ZIGBEE2MQTT_CONFIG_FRONTEND_URL=https://zigbee2mqtt.home.intra.web_domain_name
