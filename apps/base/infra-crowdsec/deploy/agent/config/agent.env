DISABLE_LOCAL_API=true
DISABLE_ONLINE_API=true
# Force DEBUG level for the container log
LEVEL_DEBUG=false
# Trace the entrypoint
DEBUG=false
LOCAL_API_URL=https://lapi.infra-crowdsec.svc.cluster.local:8080
USE_TLS=true
CACERT_FILE=/etc/ssl/crowdsec/ca.crt
CLIENT_CERT_FILE=/etc/ssl/crowdsec/agent/agent-chain.crt
CLIENT_KEY_FILE=/etc/ssl/crowdsec/agent/agent.key
COLLECTIONS=crowdsecurity/linux crowdsecurity/sshd crowdsecurity/nginx
