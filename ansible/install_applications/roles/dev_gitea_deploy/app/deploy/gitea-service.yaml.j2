apiVersion: v1
kind: Service
metadata:
  name: gitea
  namespace: {{ gitea.namespace }}
  labels:
    {{ gitea.labels_def | to_nice_yaml() | indent(4) }}
    {{ gitea.labels_desc | to_nice_yaml() | indent(4) }}
    prometheus-target: gitea
spec:
  ports:
  - port: 3000
    name: http
  selector:
    {{ gitea.labels_def | to_nice_yaml() | indent(4) }}
