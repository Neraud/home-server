APP_NAME=Heimdall
APP_ENV=production
APP_KEY=base64:{{ (inventory_hostname + '_heimdall') | to_uuid | regex_replace('-', '') | b64encode }}
APP_DEBUG=false
APP_LOG_LEVEL=info
APP_URL=https://heimdall.{{ web_base_domain }}
FORCE_HTTPS=true

DB_CONNECTION=sqlite
DB_DATABASE=app.sqlite

BROADCAST_DRIVER=log
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
QUEUE_DRIVER=database
