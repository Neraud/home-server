apiVersion: v1
kind: Service
metadata:
  name: kube-proxy
  namespace: kube-system
  labels:
    app.kubernetes.io/name: kube-proxy
    app.kubernetes.io/component: kube-proxy
    prometheus-target: kube-proxy
spec:
  selector:
    k8s-app: kube-proxy
  ports:
  - name: http-metrics
    port: 10249
    targetPort: 10249
    protocol: TCP
