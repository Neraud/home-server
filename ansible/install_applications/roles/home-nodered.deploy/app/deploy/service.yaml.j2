apiVersion: v1
kind: Service
metadata:
  name: nodered
  namespace: {{ nodered.namespace }}
  labels:
    {{ nodered.labels_def | to_nice_yaml() | indent(4) }}
    {{ nodered.labels_desc | to_nice_yaml() | indent(4) }}
spec:
  ports:
  - port: 80
    targetPort: 1880
  selector:
    {{ nodered.labels_def | to_nice_yaml() | indent(4) }}
