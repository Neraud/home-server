apiVersion: v1
kind: Secret
metadata:
  name: fluent-bit-secret
  namespace: {{ fluentbit.namespace }}
  labels:
    {{ fluentbit.labels_def | to_nice_yaml() | indent(4) }}
    {{ fluentbit.labels_desc | to_nice_yaml() | indent(4) }}
data:
  fluentd_shared_key: "{{ fluentd.shared_key | b64encode }}"
