apiVersion: v1
kind: ConfigMap
metadata:
  name: kibana
  namespace: {{ kibana.namespace }}
  labels:
    {{ kibana.labels_def | to_nice_yaml() | indent(4) }}
    {{ kibana.labels_desc | to_nice_yaml() | indent(4) }}
data:
  opensearch_dashboards.yml: |
    {{ lookup('template', role_path + '/app/config/opensearch_dashboards.yml.j2') | replace('\n', '\n    ') }}
