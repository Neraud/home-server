apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  labels:
    app: alertmanager
  name: alertmanager
  namespace: monitoring
spec:
  endpoints:
  - interval: {{ (30 * service_monitors.interval_factor | float) | int }}s
    port: web
  selector:
    matchLabels:
      alertmanager: main
      prometheus-target: alertmanager-main
