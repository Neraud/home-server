apiVersion: v1
kind: Service
metadata:
  name: pgsql
  namespace: {{ miniflux.namespace }}
  labels:
    {{ miniflux_pgsql.labels_def | to_nice_yaml() | indent(4) }}
    {{ miniflux_pgsql.labels_desc | to_nice_yaml() | indent(4) }}
spec:
  clusterIP: None
  ports:
  - port: 5432
  selector:
    {{ miniflux_pgsql.labels_def | to_nice_yaml() | indent(4) }}
