apiVersion: v1
kind: Service
metadata:
  name: zwavejs2mqtt
  namespace: {{ zwavejs2mqtt.namespace }}
  labels:
    {{ zwavejs2mqtt.labels_def | to_nice_yaml() | indent(4) }}
    {{ zwavejs2mqtt.labels_desc | to_nice_yaml() | indent(4) }}
spec:
  ports:
  - name: http
    port: 8091
    targetPort: http
  - name: ws
    port: 3000
    targetPort: ws
  selector:
    {{ zwavejs2mqtt.labels_def | to_nice_yaml() | indent(4) }}
