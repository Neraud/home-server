apiVersion: v1
kind: Service
metadata:
  name: plex
  namespace: {{ plex.namespace }}
  labels:
    {{ plex.labels_def | to_nice_yaml() | indent(4) }}
    {{ plex.labels_desc | to_nice_yaml() | indent(4) }}
spec:
  ports:
  - port: 80
    targetPort: 32400
  selector:
    {{ plex.labels_def | to_nice_yaml() | indent(4) }}
