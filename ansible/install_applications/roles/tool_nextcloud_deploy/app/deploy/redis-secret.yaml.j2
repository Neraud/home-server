apiVersion: v1
kind: Secret
metadata:
  name: redis
  namespace: {{ nextcloud.namespace }}
  labels:
    {{ nextcloud_redis.labels_def | to_nice_yaml() | indent(4) }}
    {{ nextcloud_redis.labels_desc | to_nice_yaml() | indent(4) }}
type: Opaque
data:
  redis.conf: {{ lookup('template', role_path + '/app/config/redis.conf.j2') | b64encode }}
  password: {{ paperless_redis.password | b64encode }}
