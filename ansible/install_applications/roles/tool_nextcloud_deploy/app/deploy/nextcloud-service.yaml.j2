apiVersion: v1
kind: Service
metadata:
  name: nextcloud
  namespace: {{ nextcloud.namespace }}
  labels:
    {{ nextcloud.labels_def | to_nice_yaml() | indent(4) }}
    {{ nextcloud.labels_desc | to_nice_yaml() | indent(4) }}
spec:
  ports:
  - port: 8080
    name: web
    targetPort: web
  selector:
    {{ nextcloud.labels_def | to_nice_yaml() | indent(4) }}
