watchers:

  ###################
  # Native on hosts #
  ###################
  - type: pypi
    package: ansible
    version: 7.2.0
    excludes: 
      - .*[ab][1-9]$
      - .*rc[1-9]$
      - ^[0-6]\..*
      - ^7\.[0-1]\..*


  #################################
  # Installed by ansible on hosts #
  #################################
  - type: github_release
    repo: cafebazaar/keepalived-exporter
    release: v1.2.0
  - type: github_release
    repo: kubernetes/kubernetes
    release: v1.26.1
    excludes:
      - .*(alpha|beta|rc).*
      - v1\.[0-1][0-9]\..*
      - v1\.2[0-4]\..*
  

  ########
  # Misc #
  ########
  - type: docker_registry
    repo: {{ busybox.base_image.repo | regex_replace('^docker.io$', 'registry-1.docker.io') | default('registry-1.docker.io') }}
    image: {{ busybox.base_image.name }}
    tag: {{ busybox.base_image.tag }}
    includes:
      - "^[0-9]+\\.[0-9]+\\.[0-9]$"
    excludes:
      - 1\.[0-2][0-9]\..*
      - 1\.3[0-4]\..*
  

  #########
  # Infra #
  #########
  - type: github_release
    repo: projectcalico/calico
    release: v3.25.0
    excludes:
      - v3\.1[0-9]\..*
      - v3\.2[0-4]\..*
  - type: docker_registry
    repo: ghcr.io
    image: k8snetworkplumbingwg/multus-cni
    tag: v3.9.2
    includes:
      - "^v[0-9]+\\.[0-9]+\\.[0-9]$"
    excludes:
      - v[0-2]\..*
      - v3\.[0-8](\..*)?


  ###############
  # Apps : Auth #
  ###############
