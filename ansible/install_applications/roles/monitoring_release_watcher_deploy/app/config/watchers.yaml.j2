watchers:

  ###################
  # Native on hosts #
  ###################
  - type: pypi
    package: ansible
    version: 5.7.0
    excludes: 
      - .*[ab][1-9]$
      - .*rc[1-9]$
      - ^[0-4]\..*
      - ^5\.[0-6]\..*


  #################################
  # Installed by ansible on hosts #
  #################################
  - type: github_commit
    repo: spotify/docker-gc
    branch: master
    commit: 1e4dbcf1f683670432914be6701d22a0da82a530
  - type: github_commit
    repo: gluster/gluster-prometheus
    branch: master
    commit: 3ebaacc2c690223f77fc93f5f237ab5065138660
  - type: github_release
    repo: cafebazaar/keepalived-exporter
    release: v1.2.0
  

  ########
  # Misc #
  ########
  - type: docker_registry
    repo: {{ busybox.base_image.repo | regex_replace('^docker.io$', 'registry-1.docker.io') | default('registry-1.docker.io') }}
    image: {{ busybox.base_image.name }}
    tag: {{ busybox.base_image.tag }}
    includes:
      - "^[0-9]+\\.[0-9]+\\.[0-9]$"
    excludes:
      - 1\.[0-2][0-9]\..*
      - 1\.3[0-3]\..*
  

  #########
  # Infra #
  #########
  - type: github_release
    repo: coreos/flannel
    release: v0.17.0
    excludes:
      - .*-rc[0-9]+.*
  - type: github_release
    repo: projectcalico/calico
    release: v3.23.0
    excludes:
      - v3\.1[0-9]\..*
      - v3\.2[0-2]\..*
  - type: docker_registry
    repo: ghcr.io
    image: k8snetworkplumbingwg/multus-cni
    tag: v3.8.1
    includes:
      - "^v[0-9]+\\.[0-9]+\\.[0-9]$"
    excludes:
      - v[0-2]\..*
      - v3\.[0-7](\..*)?


  ###############
  # Apps : Auth #
  ###############
