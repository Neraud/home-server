    repo: {{ lemonldap.base_image.repo | regex_replace('^docker.io$', 'registry-1.docker.io') | default('registry-1.docker.io') }}
    image: {{ lemonldap.base_image.name }}
    tag: {{ lemonldap.base_image.tag }}
    excludes:
      - latest
      - ^1\.
      - ^2\.0\.[0-9]$
  - type: docker_registry
    repo: {{ openldap.base_image.repo | regex_replace('^docker.io$', 'registry-1.docker.io') | default('registry-1.docker.io') }}
    image: {{ openldap.base_image.name }}
    tag: {{ openldap.base_image.tag }}
    includes:
      - "[0-9]+\\.[0-9]+\\.[0-9]+-amd64"
    excludes:
      - ^1\.[0-4]\.
  - name: phpLDAPadmin-php
    type: docker_registry
    repo: {{ phpldapadmin.base_image.repo | regex_replace('^docker.io$', 'registry-1.docker.io') | default('registry-1.docker.io') }}
    image: {{ phpldapadmin.base_image.name }}
    tag: {{ phpldapadmin.base_image.tag }}
    includes:
      - "7.3.[0-9]+-apache-buster"
    excludes:
      - "7.3.[0-9]-apache-buster"
      - "7.3.[0-2][0-9]-apache-buster"
  - type: github_tag
    repo: leenooks/phpLDAPadmin
    tag: {{ phpldapadmin.version }}
  
  ##############
  # Apps : Dev #
  ##############
