apiVersion: v1
kind: Secret
type: kubernetes.io/dockerconfigjson
metadata:
  name: docker-config
  namespace: cluster-secrets
  annotations:
    reflector.v1.k8s.emberstack.com/reflection-allowed: "true"
data:
  .dockerconfigjson: {{ lookup('template', 'docker_auth.json.j2', convert_data=False) | b64encode }}
